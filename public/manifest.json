{
  "name": "Minuz Ext Svelte",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "alarms",
    "browsingData",
    "contextMenus",
    "declarativeContent",
    "history",
    "idle",
    "nativeMessaging",
    "sessions",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest"
  ],
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"],
      "all_frames": true
    }
  ]
}
